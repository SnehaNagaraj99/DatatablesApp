<p id="notice"><%= notice %></p>

<h1>Ecom Orders</h1>

<table id  ='ordersTable' class='table table-striped table-sm'>
  <thead>
    <tr>
      <th>Price</th>
      <th>Item sku</th>
      <th>Purchase date</th>
      <th>Platform order num</th>
      <th>Sales channel</th>
      <th>Status</th>
      <th>Customer name</th>
      <th>Customer state</th>
      <th>Shipped at</th>
      <th>Number of items shipped</th>
      <th>Payment method</th>
    </tr>
  </thead>

  <tbody>
    <% @ecom_orders.each do |ecom_order| %>
      <tr>
        <td><%= ecom_order.price %></td>
        <td><%= ecom_order.item_sku %></td>
        <td><%= ecom_order.purchase_date %></td>
        <td><%= ecom_order.platform_order_num %></td>
        <td><%= ecom_order.sales_channel %></td>
        <td><%= ecom_order.status %></td>
        <td><%= ecom_order.customer_name %></td>
        <td><%= ecom_order.customer_state %></td>
        <td><%= ecom_order.shipped_at %></td>
        <td><%= ecom_order.number_of_items_shipped %></td>
        <td><%= ecom_order.payment_method %></td>

      </tr>
    <% end %>
  </tbody>
</table>



